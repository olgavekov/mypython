def convert_CamelCase_to_snake_case(text):
    text1 = []
    j = 0
    n = len(text)
    for i in range(1, n):
        if text[i].isupper():
            text2 = text[j:i]
            text1.append(text2.lower())
            j = i
    text2 = text[j:n]
    text1.append(text2.lower())
    text = '_'.join(text1)
    return text
